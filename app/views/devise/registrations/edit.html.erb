<div class="container-tour">
  <div class="container pt-4 pb-4">
    <div class="profile-container">
      <div class="row m-0">
        <div class="col-md-3 p-0">
          <div class="left-profile-container">
            <%= image_tag "architecture-buildings-business.jpg", width: "100%", height: "200px" %>
            <h4 class="mt-3 mb-3 mr-2 ml-2"><%= current_user.name %></h4>
            <div class="tab-profile">
              <button class="tablinks-profile"></button>
              <%= link_to t("delete_account"), registration_path(resource_name), class: "btn btn-outline-light btn-user", data: { confirm: t("sure?") }, method: :delete %>
            </div>
          </div>
        </div>
        <div class="col-md-9 p-0">
          <div class="right-profile-container">
            <div id="changePassword" class="tabcontent">
              <h3 id="h3_center"><%= t("change_infor") %></h3>
              <br>
              <%= render "form_edit" %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
