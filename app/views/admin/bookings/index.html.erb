<div class="col-lg-12 grid-margin stretch-card">
  <div class="card">
    <div class="card-body">
      <div class="table-responsive">
        <table class="table table-bordered", id="table_booking">
          <p class="btn btn-info"><%= t(".total_revenue", revenue: number_to_currency(@salary)) %>
          <thead>
            <tr>
              <th><%= t(".user_id") %></th>
              <th><%= t(".tour_id") %></th>
              <th><%= t(".quantity") %></th>
              <th><%= t(".status") %></th>
              <th><%= t(".bill") %></th>
              <th><%= t(".name") %></th>
              <th><%= t(".phone") %></th>
              <th><%= t(".address") %></th>
              <th><%= t(".note") %></th>
            </tr>
          </thead>
          <tbody>
            <%= render @bookings %>
          </tbody>
        </table>
        <%= will_paginate %>
      </div>
      <div class="container">
        <p>
          <%= t(".download") %>
          <%= link_to t(".export_excel"), admin_bookings_path(format: Settings.xls) %>
        </p>
      </div>
    </div>
  </div>
</div>
